#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules/sanity/bin/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules/sanity/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules/sanity/bin/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules/sanity/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/sanity@4.14.1_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.2.0_@sanity+schema@4_ho4n22xbmb46mue6kx3tyhgpcy/node_modules:/Users/jackgreenberg/Desktop/rank-and-rent/1031-exchange/rr-new-york-ny-1031-exchange/rr-new-york-ny-1031-exchange-frontend/sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
